    ./MainWindow.cpp \
    ./main.cpp \
    ../../auxiliary/CommandLineParser.cpp \
    ../../data/CDTTMeasurement.cpp \
    ../../data/CDTTTest.cpp \
    ../../data/MeasurementBase.cpp \
    ../../managers/ManagerBase.cpp
FORMS += ./MainWindow.ui


