    ./MainWindow.h \
    ../../data/CDTTMeasurement.h \
    ../../data/CDTTTest.h \
    ../../data/MeasurementBase.h \
    ../../data/TestBase.h \
    ../../auxiliary/CommandLineParser.h \
    ../../managers/ManagerBase.h
